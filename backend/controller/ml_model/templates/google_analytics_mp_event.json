{
  {% if unique_id_type == UniqueId.CLIENT_ID or google_analytics.in_source %}
  "clientId": "${client_id}",
  {% endif %}
  {% if unique_id_type == UniqueId.USER_ID %}
  "userId": "${user_id}",
  {% endif %}
  "nonPersonalizedAds": false,
  "events": [
    {
      "name": "${event_name}",
      "params": {
        "type": "${type}",
        "value": "${value}",
        {% if type.is_classification %}
        "score": "${score}",
        "nscore": "${normalized_score}"
        {% elif type.is_regression %}
        "revenue": "${revenue}"
        {% endif %}
      }
    }
  ]
}
